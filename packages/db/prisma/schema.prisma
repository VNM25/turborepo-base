generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  email    String?  @unique
  name     String?
  mobNo    String @unique
  password String
}

model Merchant {
  id        Int      @id @default(autoincrement())
  email     String?  @unique()
  name      String?
  auth_type AuthType
}

enum AuthType {
  Google
  Github
}
